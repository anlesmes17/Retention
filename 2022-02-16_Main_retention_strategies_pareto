SELECT DISTINCT
  MES_CIERRE,
  ESTRATEGIA_APLICADA,
  SOLUCION_FINAL,
  COUNT(*) AS CUENTA
FROM
  `gcp-bia-tmps-vtr-dev-01.gcp_temp_cr_dev_01.2022-02-02_TIQUETES_GENERALES_DE_DESCONEXIONES_T`
WHERE
  ANHO_CIERRE =2021
GROUP BY
  MES_CIERRE,
  SOLUCION_FINAL,
  ESTRATEGIA_APLICADA
ORDER BY
  MES_CIERRE ASC,
  cuenta DESC,
  ESTRATEGIA_APLICADA DESC
